<Window x:Class="f2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:f2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock FontSize="40" HorizontalAlignment="Left" Margin="245,44,0,0" TextWrapping="Wrap" Text="DOG VPN" VerticalAlignment="Top"/>
        <TextBox Name="serverName" HorizontalAlignment="Left" Height="65" Margin="224,152,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="215"/>
        <TextBox Name="userName" HorizontalAlignment="Left" Height="60" Margin="224,234,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="215"/>
        <TextBox Name="password" HorizontalAlignment="Left" Height="85" Margin="224,314,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="215"/>
        <Button Content="Connect to the dog shit VPN" HorizontalAlignment="Left" Height="95" Margin="505,29,0,0" VerticalAlignment="Top" Width="200" Click="connect"/>
        <Button Content="Disconnect from the dog water VPN" HorizontalAlignment="Left" Height="255" Margin="505,144,0,0" VerticalAlignment="Top" Width="200" Click="disconnect"/>

    </Grid>
</Window>
